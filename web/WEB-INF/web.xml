<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
<welcome-file-list>
    <welcome-file>login.html</welcome-file>
</welcome-file-list>
    
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>cn.tutetube.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>sendmessage</servlet-name>
        <servlet-class>cn.tutetube.servlet.messageservice.MessageSend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sendmessage</servlet-name>
        <url-pattern>/sendmessage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>checknickname</servlet-name>
        <servlet-class>cn.tutetube.servlet.CheckNickNameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>checknickname</servlet-name>
        <url-pattern>/checknickname</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>cn.tutetube.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>title</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.TitleListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>title</servlet-name>
        <url-pattern>/title</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addcookies</servlet-name>
        <servlet-class>cn.tutetube.servlet.cookies.AddCookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addcookies</servlet-name>
        <url-pattern>/addcookies</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>findcookies</servlet-name>
        <servlet-class>cn.tutetube.servlet.cookies.FindCookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>findcookies</servlet-name>
        <url-pattern>/findcookies</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>updatecookies</servlet-name>
        <servlet-class>cn.tutetube.servlet.cookies.UpdateCookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updatecookies</servlet-name>
        <url-pattern>/updatecookies</url-pattern>
    </servlet-mapping>



    <!--获取当前学期成绩-->
    <servlet>
        <servlet-name>getScore</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.GetScoreServerlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getScore</servlet-name>
        <url-pattern>/getScore</url-pattern>
    </servlet-mapping>

    <!--获取当前学期成绩-->
    <servlet>
        <servlet-name>getThisScore</servlet-name>
        <servlet-class>cn.tutetube.servlet.queryservlet.GetSelectSemesterScoreServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getThisScore</servlet-name>
        <url-pattern>/getThisScore</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>queryresult</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.GetQueryResultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>queryresult</servlet-name>
        <url-pattern>/queryresult</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>sInfo</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.StudentInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sInfo</servlet-name>
        <url-pattern>/sInfo</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>bindingJw</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.BindingJwServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bindingJw</servlet-name>
        <url-pattern>/bindingJw</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>cquery</servlet-name>
        <servlet-class>cn.tutetube.servlet.queryservlet.ChineseQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cquery</servlet-name>
        <url-pattern>/cquery</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>cinfo</servlet-name>
        <servlet-class>cn.tutetube.servlet.queryservlet.ChineseResultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cinfo</servlet-name>
        <url-pattern>/cinfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>bookQuery</servlet-name>
        <servlet-class>cn.tutetube.servlet.queryservlet.BookQueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bookQuery</servlet-name>
        <url-pattern>/bookQuery</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>bookInfo</servlet-name>
        <servlet-class>cn.tutetube.servlet.queryservlet.BookInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bookInfo</servlet-name>
        <url-pattern>/bookResult</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>GetEvaList</servlet-name>
        <servlet-class>cn.tutetube.servlet.TeachingEvaluation.GetEvaluationListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetEvaList</servlet-name>
        <url-pattern>/GetEvaList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Evaluation</servlet-name>
        <servlet-class>cn.tutetube.servlet.TeachingEvaluation.EvaluationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Evaluation</servlet-name>
        <url-pattern>/Evaluation</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>EvaluationDetails</servlet-name>
        <servlet-class>cn.tutetube.servlet.TeachingEvaluation.EvaluationDetailsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EvaluationDetails</servlet-name>
        <url-pattern>/EvaluationDetails</url-pattern>
    </servlet-mapping>

    <!--获取学期数-->
    <servlet>
        <servlet-name>getSemester</servlet-name>
        <servlet-class>cn.tutetube.servlet.queryservlet.GetAllSemesterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getSemester</servlet-name>
        <url-pattern>/getSemester</url-pattern>
    </servlet-mapping>

    <!--检测登陆状态-->
    <servlet>
        <servlet-name>checkcookie</servlet-name>
        <servlet-class>cn.tutetube.servlet.CheckCookiesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>checkcookie</servlet-name>
        <url-pattern>/checkcookie</url-pattern>
    </servlet-mapping>
    
    <!--选择广场模块-->
    <servlet>
        <servlet-name>selectnote</servlet-name>
        <servlet-class>cn.tutetube.servlet.publicnote.SelectNoteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>selectnote</servlet-name>
        <url-pattern>/selectnote</url-pattern>
    </servlet-mapping>

    <!--修改用户密码-->
    <servlet>
        <servlet-name>changePwd</servlet-name>
        <servlet-class>cn.tutetube.servlet.ChangePwdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>changePwd</servlet-name>
        <url-pattern>/changePwd</url-pattern>
    </servlet-mapping>

    <!--获取帖子-->
    <servlet>
        <servlet-name>getPublicNote</servlet-name>
        <servlet-class>cn.tutetube.servlet.publicnote.GetPublicNoteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getPublicNote</servlet-name>
        <url-pattern>/getPublicNote</url-pattern>
    </servlet-mapping>

    <!--获取帖子类型-->
    <servlet>
        <servlet-name>getNoteType</servlet-name>
        <servlet-class>cn.tutetube.servlet.publicnote.GetNoteTypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getNoteType</servlet-name>
        <url-pattern>/getNoteType</url-pattern>
    </servlet-mapping>
    
    <!--获取我的课程表-->
    <servlet>
        <servlet-name>getMyCourse</servlet-name>
        <servlet-class>cn.tutetube.servlet.queryservlet.GetMyCourseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getMyCourse</servlet-name>
        <url-pattern>/getMyCourse</url-pattern>
    </servlet-mapping>

    <!--发布帖子-->
    <servlet>
        <servlet-name>ReleaseNote</servlet-name>
        <servlet-class>cn.tutetube.servlet.publicnote.ReleaseNoteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReleaseNote</servlet-name>
        <url-pattern>/ReleaseNote</url-pattern>
    </servlet-mapping>
    
    <!--上传头像-->
    <servlet>
        <servlet-name>uploadHeadIcon</servlet-name>
        <servlet-class>cn.tutetube.servlet.base.UploadHeadIconServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadHeadIcon</servlet-name>
        <url-pattern>/uploadHeadIcon</url-pattern>
    </servlet-mapping>
    
    <!--获取用户app信息-->
    <servlet>
        <servlet-name>getUserInfo</servlet-name>
        <servlet-class>cn.tutetube.servlet.base.GetUserInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getUserInfo</servlet-name>
        <url-pattern>/getUserInfo</url-pattern>
    </servlet-mapping>
    
    <!--保存modulePartId临时-->
    <servlet>
        <servlet-name>SaveModulePartId</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.tute.AddModulePartIdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveModulePartId</servlet-name>
        <url-pattern>/SaveModulePartId</url-pattern>
    </servlet-mapping>

    <!--获取modulePart部分-->
    <servlet>
        <servlet-name>getModulePartDetails</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.tute.GetModulePartDetailsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getModulePartDetails</servlet-name>
        <url-pattern>/getModulePartDetails</url-pattern>
    </servlet-mapping>

    <!--通过module_id获取相应栏目-->
    <servlet>
        <servlet-name>getmodule</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.tute.GetModuleIdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getmodule</servlet-name>
        <url-pattern>/getmodule</url-pattern>
    </servlet-mapping>
    
    <!--通过module_id和college_id获取该学生学院的信息-->
    <servlet>
        <servlet-name>getCollegeNews</servlet-name>
        <servlet-class>cn.tutetube.servlet.school.tute.GetUserCollegeNewsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getCollegeNews</servlet-name>
        <url-pattern>/getCollegeNews</url-pattern>
    </servlet-mapping>
</web-app>
