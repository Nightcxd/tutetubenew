<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="cn.tutetube.bean.Module" table="module">
        <id name="module_id" type="string" column="module_id">
            <generator class="assigned"/>
        </id>
        <property name="module_name" type="string" column="module_name"/>
        <property name="university_id" type="string" column="university_id" />
        <property name="module_type" type="string" column="module_type" />
        <property name="module_description" type="string" column="module_description" />
        <property name="module_releaseTime" type="string" column="module_releaseTime" />
        <property name="college_id" type="string" column="college_id" />
    </class>

</hibernate-mapping>
