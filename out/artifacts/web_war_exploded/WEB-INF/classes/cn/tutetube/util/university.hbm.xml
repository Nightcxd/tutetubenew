<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="cn.tutetube.bean.University" table="university">
        <id name="university_id" type="string" column="university_id">
            <generator class="assigned"/>
        </id>
        <property name="university_name" type="string" column="university_name"/>
        <property name="university_type" type="string" column="university_type" />
        <property name="university_description" type="string" column="university_description" />
    </class>

</hibernate-mapping>
