<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="cn.tutetube.bean.Note" table="note">
        <id name="note_id" type="string" column="note_id">
            <generator class="assigned"/>
        </id>
        <property name="id" type="string" column="id"/>
        <property name="release_time" type="string" column="release_time" />
        <property name="note_type" type="string" column="note_type" />
        <property name="note_content" type="string" column="note_content" />
    </class>

</hibernate-mapping>
