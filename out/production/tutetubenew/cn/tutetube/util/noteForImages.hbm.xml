<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.tutetube.bean.NoteForImages" table="noteForImages">
        <id name="images_id" type="string" column="images_id">
            <generator class="assigned"/>
        </id>
        <property name="id" type="string" column="id"/>
        <property name="note_id" type="string" column="note_id"/>
        <property name="note_images" type="string" column="note_images" />
    </class>
</hibernate-mapping>
