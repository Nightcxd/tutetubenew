<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="cn.tutetube.bean.ModuleBase" table="moduleBase">
        <id name="moduleBase_id" type="string" column="moduleBase_id">
            <generator class="assigned"/>
        </id>
        <property name="modulePart_name" type="string" column="modulePart_name"/>
        <property name="modulePart_type" type="string" column="modulePart_type" />
        <property name="module_id" type="string" column="module_id" />

    </class>

</hibernate-mapping>
